key AX, sum; set sum, 0
key BX, a; set a, 1
key CX, b; set b, 2

.check_a_is_even
    key DX, n; mov n, a
    set RBX, 1; and n, RBX; sub n, RBX, RAX; brz RAX, .fib
    add sum, a

.fib
    key DX, c; mov c, b
    add b, a
    mov a, c
    set RAX, 35; sub RAX, a; brp RAX, .check_a_is_even

write sum
end
