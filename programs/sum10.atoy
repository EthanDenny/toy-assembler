KEY AX, sum         // sum = 0
SET sum, 0

KEY BX, i           // i = 1
SET i, 1


.loop               // .loop
    ADD sum, i      //     sum += i

    SET RBX, 1      //     i += 1
    ADD i, RBX

    SET RAX, 11     //     if (11 - i == 0) goto .out
    SUB RAX, i
    BZ RAX, .out
    
    JUMP .loop      //     goto .loop


.out                // .out
    WRITE sum       //     print sum
    END


